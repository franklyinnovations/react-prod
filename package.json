{
  "private": true,
  "name": "pateast-web",
  "version": "4.1.0",
  "homepage": "https://www.pateast.co/",
  "watch": {
    "dev:server": "build/*.js"
  },
  "scripts": {
    "clean:js": "rimraf public/js/dist/*",
    "clean:css": "rimraf public/css/main*.css && rimraf public/css/app*.css && rimraf public/css/*.map",
    "clean:assets": "rimraf public/assets/*",
    "clean:server": "rimraf build",
    "clean:build": "rimraf build",
    "clean": "run-s clean:build clean:server clean:css clean:js clean:assets",
    "hot:client": "cross-env NODE_ENV=development webpack-dev-server --config tools/webpack.client.js --hot",
    "hot:server": "cross-env NODE_ENV=development webpack --colors --watch --config tools/webpack.server.js",
    "dev:server": "cross-env NODE_ENV=development node build/server.js",
    "dev:server:wait": "just-wait -p \"build/server.js\" --timeout 120 && npm run start-watcher",
    "start-watcher": "npm-watch",
    "watch-build-server": "run-s clean:build hot:server",
    "dev": "rimraf build && concurrently --kill-others \"npm run watch-build-server\" \"npm run hot:client\" \"npm run dev:server:wait\"",
    "dev:rtl": "cross-env RTL=true run-s dev",
    "build:server": "cross-env NO_OUTPUT_PATH=true NODE_ENV=production webpack --progress --colors --display-error-details --config tools/webpack.server.js",
    "build:client": "cross-env webpack --progress --colors --display-error-details --config tools/webpack.client.js",
    "bundle:server": "run-s clean:build clean:server build:server",
    "bundle:client": "run-s clean:css clean:js clean:assets build:client",
    "prod:server": "cross-env NODE_ENV=production node build/server.js",
    "build:prod": "cross-env NODE_ENV=production run-s bundle:client bundle:server",
    "build:prod:rtl": "cross-env NODE_ENV=production RTL=true run-s bundle:client bundle:server",
    "prod": "run-s build:prod prod:server",
    "prod:rtl": "run-s build:prod:rtl prod:server",
    "test": "mocha --compilers :./tools/babel-setup.js --recursive --watch ./test/*.js",
    "start": "node build/server.js"
  },
  "author": "www.pateast.co",
  "devDependencies": {
    "@sketchpixy/just-wait": "^1.0.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cache-loader": "^1.2.2",
    "concurrently": "^3.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "deepmerge": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "npm-watch": "^0.3.0",
    "postcss-loader": "^2.1.6",
    "rimraf": "^2.6.2",
    "rtlcss": "^2.4.0",
    "sass-loader": "^7.0.3",
    "script-loader": "^0.7.2",
    "style-loader": "^0.20.3",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "autoprefixer": "^8.6.5",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "c3": "^0.4.23",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.6.1",
    "emojione_minimal": "^2.1.2",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "es6-promise": "^3.3.1",
    "exceljs": "^0.8.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "filereader-stream": "^1.0.0",
    "fullcalendar": "^3.9.0",
    "html-pdf": "^2.2.0",
    "http-proxy": "^1.17.0",
    "immutable": "^3.8.2",
    "jquery": "^3.3.1",
    "lodash-compat": "^3.10.2",
    "messenger-hubspot": "^1.5.0-b2",
    "moment": "^2.22.2",
    "papaparse": "^4.5.0",
    "phantomjs": "^2.1.7",
    "prop-types": "^15.6.2",
    "pug": "^2.0.0-beta3",
    "qrcode.react": "^0.8.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-c3js": "^0.1.20",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-overlays": "^0.8.3",
    "react-prop-types": "^0.4.0",
    "react-redux": "^4.4.9",
    "react-router": "^3.2.1",
    "react-router-redux": "^4.0.8",
    "react-select": "^1.0.0-rc.10",
    "react-tinymce": "^0.7.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.4.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.87.0",
    "request-frame": "^1.5.3",
    "session-file-store": "^1.1.2",
    "socket.io-client": "^2.1.1",
    "sortablejs": "^1.7.0",
    "trumbowyg": "^2.10.0",
    "vex-js": "^4.1.0"
  }
}
